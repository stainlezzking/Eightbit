<style>
    
/* ######### preloader ########## */
.preloader {
    display: flex;
    justify-content: center;
    align-items: center;
    position: fixed;
    background-color: black;
    width: 100%;
    height: 100%;
    top: 0px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    z-index: 10000;
}
.preloader .spinner {
    display: block;
    width: 150px;
    height: 150px;
    border-top: 4px solid var(--light-green);
    border-bottom: 4px solid var(--background-light);
    border-radius: 50%;
    animation: spin_rotate infinite linear;
    animation-duration: 1s;
    position: absolute;
}
.preloader .spinner::after{
    content: "";
    display: block;
    position: relative;
    background-color: black;
    width: 40px;
    height: 100px;
    top: calc(50% - 50px );
    left: 120px;
}
.preloader .spinner::before {
    content: "";
    display: block;
    position: absolute;
    background-color: black;
    width: 40px;
    height: 100px;
    top: calc(50% - 50px );
}
.bitcoin_map_logo svg{
    height: 100px;
    z-index: 100001;
    transform: rotate(-13deg);
}
.bitcoin_path {
    stroke-dasharray: 1600;
    stroke-dashoffset: 1600;
    stroke-width: 4;
    stroke-linecap: round;
    -webkit-animation: dash 3s linear infinite;
    animation: draw_path 3s linear infinite;
    fill-opacity: 0;
    stroke: var(--orange);
    stroke-width: 8px;
  }
@keyframes draw_path {
    100% {
        stroke-dashoffset: 0;
    }
}
@keyframes spin_rotate {
    from {
        transform: rotate(0deg);
    }
    to {
        transform: rotate(360deg);
    }
}
/* ############### end_0f_preloader_section ######## */
</style>

